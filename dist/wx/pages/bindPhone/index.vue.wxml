<template name="2f11864e">
  <view class="_div data-v-00d0b4cb pages_bind_phone">
    <view class="_div data-v-00d0b4cb bind_line">
      <view class="_div data-v-00d0b4cb bind_label"> 手机号码 </view> <input class="_input data-v-00d0b4cb phone_number" type="number" maxlength="11" placeholder="请输入手机号" placeholder-style="font-size:30rpx;color:rgba(202,202,202,1);" value="{{phoneNum}}" bindinput="handleProxy" data-eventid="{{'0'}}" data-comkey="{{$k}}" /> <label class="_span data-v-00d0b4cb code_click" bindtap="handleProxy" data-eventid="{{'1'}}" data-comkey="{{$k}}">{{time}}</label>
    </view>
    <view class="_div data-v-00d0b4cb bind_line">
      <view class="_div data-v-00d0b4cb bind_label"> 验证码 </view> <input class="_input data-v-00d0b4cb phone_number" type="number" maxlength="6" placeholder="输入6位验证码" placeholder-style="font-size:30rpx;color:rgba(202,202,202,1);" value="{{verifyCode}}" bindinput="handleProxy" data-eventid="{{'2'}}" data-comkey="{{$k}}" />
    </view>
    <view class="_div data-v-00d0b4cb finish_bind {{active ? 'active' : ''}}" bindtap="handleProxy" data-eventid="{{'3'}}" data-comkey="{{$k}}"> 完成 </view>
  </view>
</template>